<?xml version="1.0" encoding="utf-8"?>
<config>
	<name>ZOOcart</name>
	<params>
		<param name="enable_cart" type="radio" default="0" label="PLG_ZOOCART_CONFIG_ENABLE_CART" description="PLG_ZOOCART_CONFIG_ENABLE_CART_DESC">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<param name="show_price_with_tax" type="hidden" default="1" />
		<param name="default_tax_class" type="hidden" default="0" />
		<param name="billing_address_type" type="hidden" default="billing" />
		<param name="load_bootstrap" type="hidden" default="1" />
		<param name="vies_validation" type="hidden" default="1" />
		<param name="vies_validation_hard" type="hidden" default="0" />
